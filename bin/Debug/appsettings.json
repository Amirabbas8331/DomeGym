{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Host": "localhost",
    "Port": 8500,
    "Discovery": {
      "ServiceName": "GymManagement.Api",
      "Hostname": "localhost",
      "Port": 7137
    }
  },
  "ReverseProxy": {
    "Routes": {
      "GymRoute": {
        "ClusterId": "GymManagement-Cluster",
        "RateLimiterPolicy": "fixed",
        "Match": {
          "Path": "/Subscriptions/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "{**catch-all}" }
        ]
      }
    },
    "Clusters": {
      "GymManagement-Cluster": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7137"
          }
        }
      }
    }
  }
}
